<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
        <RootNamespace>DWPodcastFeed</RootNamespace>
    </PropertyGroup>
    
    

    <ItemGroup>
      <Content Include="..\.dockerignore">
        <Link>.dockerignore</Link>
      </Content>
    </ItemGroup>
    
    

    <ItemGroup>
        <PackageReference Include="Auth0.AuthenticationApi" Version="7.26.1" />
        <PackageReference Include="Microsoft.Playwright" Version="1.42.0" />
        <PackageReference Include="Flurl.Http" Version="3.2.4" />
        <PackageReference Include="GraphQL.Client" Version="5.1.0" />
        <PackageReference Include="GraphQL.Client.Serializer.Newtonsoft" Version="5.1.0" />
    </ItemGroup>

</Project>
